{
  "expo": {
    "name": "Yungle",
    "slug": "Yungle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/yungle.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash":{
      "resizeMode": "cover",
      "backgroundColor":"#000000"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "package": "com.Yungle.group2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/yungle.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Yungle.group2"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/yungle.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/yungle.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
